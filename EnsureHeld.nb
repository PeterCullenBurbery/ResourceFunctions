Cell[BoxData[
 RowBox[{
  TemplateBox[{"x", "x", 
    RowBox[{"-", "\[Infinity]"}], "\[Infinity]"},
   "InactiveIntegrate",
   DisplayFunction->(RowBox[{
      SubsuperscriptBox[
       StyleBox["\[Integral]", "Inactive"], #3, #4], 
      RowBox[{#, 
        RowBox[{
          StyleBox["\[DifferentialD]", "Inactive"], #2}]}]}]& ),
   InterpretationFunction->(RowBox[{
      RowBox[{"Inactive", "[", "Integrate", "]"}], "[", 
      RowBox[{#, ",", 
        RowBox[{"{", 
          RowBox[{#2, ",", #3, ",", #4}], "}"}]}], "]"}]& )], " ", 
  TemplateBox[{
    SuperscriptBox["n", "3"], "n", "1", "\[Infinity]"},
   "InactiveSum",
   DisplayFunction->(RowBox[{
      UnderoverscriptBox[
       StyleBox["\[Sum]", "Inactive"], 
       RowBox[{#2, "=", #3}], #4], #}]& ),
   InterpretationFunction->(RowBox[{
      RowBox[{"Inactive", "[", "Sum", "]"}], "[", 
      RowBox[{#, ",", 
        RowBox[{"{", 
          RowBox[{#2, ",", #3, ",", #4}], "}"}]}], "]"}]& )]}]], "Output",
 TaggingRules->{},
