Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "Function", " ", "to", " ", "ensure", " ", "that", " ", "unheld", " ", 
    "expressions", " ", "are", " ", "held"}], "*)"}], 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"EnsureHeld", "::", "usage"}], "=", 
     "\"\<EnsureHeld[input] checks whether the input is an unheld expression. \
If it is, it holds the expression. If the input is already a held expression, \
it returns the input unchanged.\>\""}], ";"}], "\n", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
    "Function", " ", "to", " ", "check", " ", "if", " ", "an", " ", 
     "expression", " ", "is", " ", "held"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"IsHeld", "::", "usage"}], "=", 
     "\"\<IsHeld[input] returns True if the input is a held expression, and \
False otherwise. A held expression is an expression that is held by Hold or \
HoldForm.\>\""}], ";"}], "\n", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
    "Function", " ", "to", " ", "find", " ", "the", " ", "head", " ", "of", 
     " ", "an", " ", "expression", " ", "without", " ", "evaluating", " ", 
     "it"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"UnevaluatedHead", "::", "usage"}], "=", 
     "\"\<UnevaluatedHead[input] gives the head of the input without \
evaluating the input. This is useful when you need to inspect the structure \
of an expression without causing side effects due to evaluation.\>\""}], 
    ";"}], "\n"}]}]], "Input",
