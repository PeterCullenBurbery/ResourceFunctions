Cell[BoxData[{
 RowBox[{"ClearAll", "[", "EnsureHeld", "]"}], "\n", 
 RowBox[{
  RowBox[{"SetAttributes", "[", 
   RowBox[{"EnsureHeld", ",", 
    RowBox[{"{", "HoldAll", "}"}]}], "]"}], "\n"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"EnsureHeld", "[", "input_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "placeholder", "}"}], ",", 
     RowBox[{
      RowBox[{"placeholder", "=", 
       RowBox[{"Unique", "[", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"Which", "[", 
       RowBox[{
        RowBox[{"IsHeld", "[", "input", "]"}], ",", "input", ",", 
        RowBox[{
         RowBox[{"Not", "[", 
          RowBox[{"IsHeld", "[", "input", "]"}], "]"}], "&&", 
         RowBox[{
          RowBox[{"UnevaluatedHead", "[", "input", "]"}], "===", "Symbol"}], "&&", 
         RowBox[{"MatchQ", "[", 
          RowBox[{
           RowBox[{"Head", "[", "input", "]"}], ",", 
           RowBox[{"Hold", "|", "HoldForm"}]}], "]"}]}], ",", "input", ",", 
        RowBox[{
         RowBox[{"Not", "[", 
          RowBox[{"IsHeld", "[", "input", "]"}], "]"}], "&&", 
         RowBox[{
          RowBox[{"UnevaluatedHead", "[", "input", "]"}], "===", "Symbol"}], "&&", 
         RowBox[{"Not", "[", 
          RowBox[{"MatchQ", "[", 
           RowBox[{
            RowBox[{"Head", "[", "input", "]"}], ",", 
            RowBox[{"Hold", "|", "HoldForm"}]}], "]"}], "]"}]}], ",", 
        RowBox[{
         RowBox[{"placeholder", "[", 
          RowBox[{"Evaluate", "[", "input", "]"}], "]"}], "/.", " ", 
         RowBox[{"{", 
          RowBox[{"placeholder", "->", "Hold"}], "}"}]}], ",", "True", ",", 
        RowBox[{"Hold", "[", "input", "]"}]}], "]"}]}]}], "]"}]}], 
  "\[IndentingNewLine]", "\n", 
  RowBox[{"(*", 
   RowBox[{
   "Defines", " ", "a", " ", "function", " ", "to", " ", "check", " ", "if", 
    " ", "an", " ", "expression", " ", "is", " ", "held"}], 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{"ClearAll", "[", "IsHeld", "]"}], "\n", 
 RowBox[{"SetAttributes", "[", 
  RowBox[{"IsHeld", ",", "HoldAll"}], "]"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"IsHeld", "[", "input_", "]"}], ":=", 
   RowBox[{"MatchQ", "[", 
    RowBox[{
     RowBox[{"UnevaluatedHead", "[", "input", "]"}], ",", 
     RowBox[{"HoldForm", "|", "Hold"}]}], "]"}]}], "\[IndentingNewLine]", 
  "\n", 
  RowBox[{"(*", 
   RowBox[{
   "Defines", " ", "a", " ", "function", " ", "to", " ", "find", " ", "the", 
